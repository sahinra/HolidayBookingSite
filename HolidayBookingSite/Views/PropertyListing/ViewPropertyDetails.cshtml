<h4> Property Details</h4>

<div class="card">
    <div class="card-body">
        <h4 class="card-title">@Model.Name</h4>
        <h4 class="card-subtitle mb-2 text-muted">Booking Calendar</h4>
        <div class="input-group">
            <input type="text" id="datepicker" class="form-control" placeholder="Select booking date">
            <div class="input-group-append">
                <span class="input-group-text" id="basic-addon2">
                    <i class="fas fa-calendar-alt"></i>
                </span>
            </div>
        </div>
    </div>
</div>

@section scripts {
    <script>
        $(function () {
            $("#datepicker").datepicker();
        });
    </script>
}