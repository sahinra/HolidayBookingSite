@model SearchMenuModel

<form asp-controller="PropertyListing" asp-action="ListAvailable">
    <label for="startDate">Start Date:</label>
    <input type="date" id="startDate" name="start" value="@Model.StartDate.ToString("yyyy-MM-dd")" required>

    <label for="endDate">End Date:</label>
    <input type="date" id="endDate" name="end" value="@Model.EndDate.ToString("yyyy-MM-dd")" required>

    <button type="submit">Search</button>
</form>
